RewriteEngine On
RewriteCond %{HTTP_HOST} !^www\.id523a\.com [NC]
RewriteCond %{HTTP_HOST} !^$
RewriteRule ^/?(.*) http://www.id523a.com/$1 [L,R=301,NE] 
ErrorDocument 404 http://id523a.com/?p=error404
ErrorDocument 403 http://id523a.com/?p=error403
<FilesMatch "License\.txt|README\.md">
        Order Deny,Allow
        Deny from all
</FilesMatch>